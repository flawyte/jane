<entity name="Order" plural="Orders">
  <attributes>
    <attribute name="id" type="Integer" primary-key="true" />
    <attribute name="date" type="Date" default="DATE()" />
    <attribute name="time" type="Time" default="TIME()" />
  </attributes>
  <references>
    <reference entity="Product" attribute="id" as="product" />
    <reference entity="User" attribute="id" as="user" />
  </references>
</entity>
